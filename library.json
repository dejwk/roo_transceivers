{
    "name": "roo_transceivers",
    "description": "Detection and aggregation of arbitrary transceiver devices, supporting arbitrary sensors and actuators.",
    "keywords": [
        "relay",
        "switch",
        "transceiver",
        "sensor",
        "actuator",
        "smart home"
    ],
    "authors": [
        {
            "name": "Dejwk",
            "email": "dejwk.oss@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_transceivers"
    },
    "version": "1.0.3",
    "homepage": "https://github.com/dejwk/roo_transceivers",
    "frameworks": [
        "Arduino"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<benchmarks> -<doc> -<test>"
    },
    "examples": [
        "examples/**/*.ino"
    ],
    "dependencies": {
        "dejwk/roo_logging": ">=1.0.4",
        "dejwk/roo_collections": ">=1.1.6",
        "dejwk/roo_prefs": ">=1.0.3",
        "dejwk/roo_threads": ">=1.1.2",
        "dejwk/roo_time": ">0",
        "nanopb/nanopb": ">=0.4.9"
    }
}
